{
  "manifest_version": 3,
  "name": "Talentlytica Coda",
  "version": "1.0.0",
  "icons": {
    "16": "talentlytica.png",
    "48": "talentlytica.png",
    "128": "talentlytica.png"
  },
  "description": "Talentlytica Coda Extentions Time Tracker Workflow",
  "action": {
    "default_popup": "popup.html",
    "default_title": "Talentlytica Coda",
    "default_icon": "talentlytica.png"
  },
  "permissions": [
    "storage"
  ],
  "options_page": "options.html",
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "contentScript.js"
      ]
    }
  ]
}